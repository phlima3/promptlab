generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Job {
  id        String   @id @default(cuid())
  status    String   @default("queued")
  input     String
  output    String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
